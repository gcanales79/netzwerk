<!DOCTYPE html>
<html lang="en">

<head>
    {{#each ampDatos}}
    <script type="application/ld+json">
    {
        "@context":"https://schema.org",
        "@type":"NewsArticle",
        "headline":{{{json headline}}},
        "image":{
            "@type":"ImageObject",
            "url":{{{json image}}}
        },
        "author":{
            "@type":"Person",
            "name": "Gustavo A. Canales R."
        }, 
        "publisher":{
            "@type":"Organization".
            "name":"Netzwerk",
            "logo":{
                "@type":"ImageObject",
                "url":"https://netzwerk.mx/assets/dist/img/Logo_netzwerk.webp"
            }
        },
        "datePublished":{{{ampDate datePublished}}},
        "dateModified":{{{ampDate datePublished}}}
    }
    </script>
    {{/each}}
    <script defer id="Cookiebot" src="https://consent.cookiebot.com/uc.js"
        data-cbid="a6cf51a1-e1a9-480f-8901-f428a26ad4b2" data-blockingmode="auto" type="text/javascript"></script>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{metaTags.title}}</title>

    <script defer src="https://kit.fontawesome.com/28f98ade22.js" crossorigin="anonymous"></script>

    <link
        href="https://fonts.googleapis.com/css?family=Raleway:300,400,400i,500,600,700,700i&amp;subset=cyrillic,greek-ext,latin-ext"
        rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Stalemate&amp;subset=latin-ext" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Spectral+SC:400,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700" rel="stylesheet">

    {{#each bootStyle}}
    <link rel="stylesheet" href="{{href}}"
        integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">
    {{/each}}

    <link rel="preload" href="/assets/dist/css/style.min.css" as="style"
        onload="this.onload=null;this.rel='stylesheet'">


    {{#each tiny}}
    <script defer src="{{src}}" referrerpolicy="{{referrerpolicy}}"></script>
    {{/each}}
    <!--link href="https://cdn.jsdelivr.net/npm/froala-editor@3.1.0/css/froala_editor.pkgd.min.css" rel="stylesheet"
        type="text/css" /-->

    <!--link href="https://cdn.jsdelivr.net/npm/froala-editor/css/froala_style.min.css" rel="stylesheet" type="text/css" /-->
    <!--link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />
    <link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
    <link href="/assets/dist/css/{{style}}" rel="stylesheet"-->



    <link rel="canonical" href="https://netzwerk.mx{{url}}" />

    {{#each style}}
    <link rel="stylesheet" href="{{style}}" />
    {{/each}}

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-132709170-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-132709170-1');
    </script>


    <meta name="description" content="{{metaTags.description}}" />
    <meta name="keywords" content="{{metaTags.keywords}}" />
    <meta name="robots" content="index,follow" />
   
    <meta property="og:type" content="article" />
    <meta property="og:url" content="{{metaTags.url}}" />
    <meta property="og:title" content="{{metaTags.twitterTitle}}" />
    <meta property="og:description" content="{{metaTags.twitterDescription}}" />
    <meta property="article:author" content="{{metaTags.creator}}" />
    <meta property="article:published_date" content="{{metaDate metaTags.date}}" />
    <meta property="og:image" content="{{metaTags.image}}" />
    <!--Twiiter MetaTags-->
    <meta name="twitter:card" content="{{metaTags.cardType}}" />
    <meta name="twitter:title" content="{{metaTags.twitterTitle}}" />
    <meta name="twitter:description" content="{{metaTags.twitterDescription}}" />
    <meta name="twitter:site" content="{{metaTags.site}}" />
    <meta name="twitter:creator" content="{{metaTags.creator}}" />
    <meta name="twitter:image" content="{{metaTags.image}}" />
    <script data-ad-client="ca-pub-5011894547490066" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
</head>

<body>
    <header class="general-header header-layout-two">
        <div class="general-header-inner">
            <div class="header-top-wrapper">
                <div class="header-top-inner">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                <!--div class="search">
                                    <i class="fa fa-search" aria-hidden="true"></i>
                                    <input type="search" placeholder="Search ..........">
                                </div-->
                            </div>
                            <!-- // col -->
                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                <div class="social-networks">
                                    {{>socialmedia}}
                                </div>
                                <!-- // social-networks -->
                            </div>
                            <!-- // col -->
                        </div>
                        <!-- // row -->
                    </div>
                    <!-- // container -->
                </div>
                <!-- // header-top-inner -->
            </div>
            <!-- // header-top-wrapper -->
            <div class="navigation-layout-two-wrapper">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-8">
                            <div class="site-info">
                                <img src="/assets/dist/img/netzwerk_blanco.webp" class="lazyload"
                                    alt="Netzwerk Logo Blanco">
                            </div>
                            <!-- // site-info -->
                        </div>
                        <!-- // col -->
                        <div class="col-lg-9 col-md-9 col-sm-6 col-xs-4 position_static">
                            <nav class="main-nav layout-two">
                                <div id="main-nav" {{#if menu}}class="stellarnav hide-libro" {{else}}class="stellarnav"
                                    {{/if}}>
                                    <!-- class=stellarnav -->
                                    <ul style="list-style:none">
                                        <li class="nav-item"><a class="fs-2" href="/">Blog</i></a></li>
                                        <li class="nav-item"><a class="fs-2" href="/libros">Libros</i></a></li>

                                    </ul>
                                </div>
                                <!-- .stellar-nav -->
                            </nav>
                        </div>
                        <!-- // col -->
                    </div>
                    <!-- // row -->
                </div>
                <!-- // container -->
            </div>
            <!-- // navigation-layout-two-wrapper -->
        </div>
        <!-- // general-header-inner -->
    </header>
    {{{body}}}
    <footer class="general-footer">
        <div class="footer-mask" style="background-color:#1f1f23"></div>
        <div class="footer-inner">
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-lg-4 col-sm-12 col-xs-12 footer-block">
                        <div class="">
                            <div class="footer-widget-content about-widget">
                                <div class="widget-title">
                                    <h2>About Me</h2>
                                </div>
                                <div class="widget-about-site-logo">
                                    <img src="/assets/dist/img/Logo_netzwerk.webp" alt="Netzwerk Logo Negro" />
                                </div>
                                <div class="widget-content">
                                    <p></p>
                                </div>
                                <!-- // widget-content -->
                                <div class="social-networks">
                                    {{>socialmedia}}
                                </div>
                            </div>
                            <!-- // footer-widget-content -->
                        </div>
                        <!-- // footer block -->
                    </div>
                    <!-- // col -->
                    <div class="col-md-4 col-lg-4 col-sm-12 col-xs-12 footer-block">
                        <div class="">
                            <div class="footer-widget-content">
                                <div class="widget-title">
                                    <h2>Navigation</h2>
                                </div>
                                <div class="widget-content">
                                    <ul class="widget-category-listings">
                                        <li><a href="/">Home</a></li>

                                    </ul>
                                </div>
                                <!-- // widget-content -->
                            </div>
                            <!-- // footer-widget-content -->
                        </div>
                        <!-- // footer block -->
                    </div>
                    <!-- // col -->
                    <div class="col-md-4 col-lg-4 col-sm-12 col-xs-12 footer-block">
                        {{>facebook}}
                    </div>
                    <!-- // col -->
                </div>
                <!-- // row -->

                <!-- // copyright-and-nav-row -->
            </div>
            <!-- // container -->
        </div>
        <!-- // footer inner -->
    </footer>
    <!-- jQuery CDN - Slim version (=without AJAX) -->

    <div id="fb-root"></div>
    <script defer crossorigin="anonymous"
        src="https://connect.facebook.net/es_LA/sdk.js#xfbml=1&version=v9.0&appId=212194213188545&autoLogAppEvents=1"
        nonce="dUlskvyb"></script>
    <!--script src="https://code.jquery.com/jquery.js"></script-->
    <script src="/assets/dist/js/lazysizes.min.js" async=""></script>
    {{#each scriptInicial}}
    <script defer src="{{jsfile}}"></script>
    {{/each}}
    <script defer type="text/javascript" src="/assets/dist/js/bundle.min.js"></script>
    <script defer type="text/javascript" src="/assets/dist/js/custom.js"></script>

    {{#each jsarchivo}}
    <script defer src="{{jsfile}}" integrity="{{integrity}}" crossorigin="{{crossorigin}}"></script>
    {{/each}}
    <!--script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.6/umd/popper.min.js"
        integrity="sha384-wHAiFfRlMFy6i5SRaxvfOCifBUQy1xHdJ/yoi7FRNXMRBu5WHdZYu1hA6ZOblgut"
        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js"
        integrity="sha384-B0UglyR+jN6CkvvICOB2joaf5I4l3gm9GU6Hc1og6Ls7i6U/mkkaduKaBhlAXv9k"
        crossorigin="anonymous"></script-->
    <!--script type="text/javascript" src="https://cdn.jsdelivr.net/npm/froala-editor@3.1.0/js/froala_editor.pkgd.min.js"></script-->

    {{#each jsfile}}
    <script defer src="{{jsfile}}"></script>
    {{/each}}
    <!-- Twitter universal website tag code -->
